account cms

# CMS_SMTP_SERVER
host smtp.gmail.com
# CMS_SMTP_SERVER
port 587
auth off
# CMS_SMTP_USERNAME
user example
# CMS_SMTP_PASSWORD
password password
# CMS_SMTP_USE_TLS
tls on
# CMS_SMTP_USE_STARTTLS
tls_starttls on
# CMS_SMTP_REWRITE_DOMAIN
maildomain gmail.com
# CMS_SMTP_HOSTNAME
domain gmail.com
from cms@example.org

account default : cms
